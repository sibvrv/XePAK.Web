    "@storybook/react": "^6.2.9",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-import": "^2.23.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0"
